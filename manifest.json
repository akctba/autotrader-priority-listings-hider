{
  "manifest_version": 3,
  "name": "AutoTrader Priority Listings Hider",
  "version": "1.0",
  "description": "Hides vehicles under Priority Listings on AutoTrader.ca",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.autotrader.ca/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": ["storage"]
}